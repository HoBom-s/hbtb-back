{
  "root": true,
  "env": {
    "node": true,
    "browser": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "no-var": ["error"]
  }
}
